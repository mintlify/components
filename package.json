{
  "name": "@mintlify/components",
  "version": "0.3.15",
  "description": "Open-source library of UI components made with React and TailwindCSS.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "style": "./dist/main.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/mintlify/components"
  },
  "keywords": [
    "react",
    "tailwindcss"
  ],
  "license": "MIT",
  "scripts": {
