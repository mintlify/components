{
  "name": "@mintlify/components",
  "version": "1.0.3",
  "description": "Mintlify open-source UI components",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "*.css"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mintlify/components.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/mintlify/components/issues"
  },
  "homepage": "https://github.com/mintlify/components#readme",
  "license": "MIT",
  "author": "Mintlify, Inc.",
  "keywords": [
    "react",
    "components",
    "ui",
    "mintlify",
    "tailwindcss"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build && pnpm build:css",
    "build:css": "npx @tailwindcss/cli -i ./src/styles.css -o ./dist/styles.css --minify",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "lint:check": "biome check .",
    "lint:fix": "biome check --write .",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@base-ui/react": "1.1.0",
    "@shikijs/transformers": "3.21.0",
    "@sindresorhus/slugify": "2.2.1",
    "clsx": "2.1.1",
    "color": "4.2.3",
    "comlink": "4.4.2",
    "hast": "1.0.0",
    "lodash": "4.17.23",
    "lucide-react": "0.453.0",
    "mermaid": "11.12.2",
    "react-use-rect": "2.0.6",
    "shiki": "3.21.0",
    "tailwind-merge": "2.6.0"
  },
  "devDependencies": {
    "@shikijs/types": "3.21.0",
    "@storybook/addon-docs": "10.1.11",
    "@storybook/react-vite": "10.1.11",
    "@tailwindcss/cli": "4.1.18",
    "@tailwindcss/postcss": "4.1.18",
    "@tailwindcss/typography": "0.5.19",
    "@tailwindcss/vite": "4.1.18",
    "@types/color": "4.2.0",
    "@types/hast": "3.0.4",
    "@types/lodash": "4.17.23",
    "@types/node": "22.19.1",
    "@types/react": "18.3.27",
    "@types/react-dom": "18.3.7",
    "@typescript-eslint/eslint-plugin": "8.47.0",
    "@typescript-eslint/parser": "8.47.0",
    "@vitejs/plugin-react": "4.7.0",
    "globals": "15.15.0",
    "mini-svg-data-uri": "1.4.4",
    "next-themes": "0.4.6",
    "postcss": "8.5.6",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rimraf": "6.0.1",
    "storybook": "10.1.11",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "ultracite": "7.0.12",
    "vite": "6.4.1",
    "vite-plugin-dts": "4.5.4"
  }
}
